<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>
$(document).ready(function(){
    	var JSONItems = [];
		$.get( "http://127.0.0.1:8887/data.json", function( data){
		  JSONItems = data;
		  console.log(JSONItems[0]);
		  });
		
			
	 
   $.getJSON("http://127.0.0.1:8887/data.json", function(result){
		
		   $.each(result, function(i, elem){
			if(i<1){
			$('table.tftable').append('<tr><td>'+elem.Merchant_Name_Input+'</td><td>'+elem.Address_Input +'</td><td>'+elem.State_Input +'</td><td>'+elem.City_Input +'</td><td>'+elem.Zip_Input +'</td></tr>');
                }
            });
        });


		$.getJSON("http://127.0.0.1:8887/data.json", function(result){
            $.each(result, function(i, elem){
			if(i<2){
			$('table.tftable1').append('<tr><td>'+elem.Merchant_Name_Input+'</td><td>'+elem.Address_Input +'</td><td>'+elem.State_Input +'</td><td>'+elem.City_Input +'</td><td>'+elem.Zip_Input +'</td><td>'+elem.Zip_Input +'</td><td width="3%"><input type="radio" name="yesno" class="increment-btn'+i+'"/></td></tr>');
			
                }
            });
        });
		
	var $button11 = $('.increment-btn0');
	var $button22 = $('.increment-btn1');
	var $button33 = $('.increment-btn3');
	
	var $counter1 = $('.counter1');
	var $counter2 = $('.counter2');
	var $counter3 = $('.counter3');

$button11.click(function(){
  $counter1.val( parseInt($counter1.val()) + 1 );
  });
$button22.click(function(){
  $counter2.val( parseInt($counter2.val()) + 1 );
  });
$button33.click(function(){
  $counter3.val( parseInt($counter3.val()) + 1 );
  });
    
	
	$button33.click(function(){
	
		var par = $('table.tftable1 td').parent(); //tr
		par.remove();
        var tr;
			for (var i = 0; i < JSONItems.length-(JSONItems.length-2); i++) {
			  tr = $('<tr/>');
			  tr.append("<td>" + JSONItems[i].Merchant_Name_Input + "</td>");
			  tr.append("<td>" + JSONItems[i].Address_Input + "</td>");
			  tr.append("<td>" + JSONItems[i].State_Input + "</td>");
			  tr.append("<td>" + JSONItems[i].City_Input + "</td>");
			  tr.append("<td>" + JSONItems[i].Zip_Input + "</td>");
			  $('.tftable1').append(tr);
			  JSONItems.splice(i, 1);
			}
    });
});

  
</script>
